{
    "name": "wcomnisky/flagship-php-sdk",
    "description": "AB Tasty Flagship.io PHP SDK (non-official)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "William Comnisky",
            "email": "w.comnisky@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "symfony/http-client": "^4.4 || ^5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Wcomnisky\\Flagship\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wcomnisky\\Flagship\\UnitTests\\": "test/unit",
            "Wcomnisky\\Flagship\\FunctionalTests\\": "test/functional"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
